        <li><a href="#" target="_blank" rel="noreferrer" class="footer-link"><%= gettext("Product 2") %></a></li>
        <li><a href="#" target="_blank" rel="noreferrer" class="footer-link"><%= gettext("Product 3") %></a></li>


